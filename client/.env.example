# Dubai Luxury Cars - Client Environment Variables
# Copy this file to .env and fill in your values

# API connection URL (required)
REACT_APP_API_URL=http://localhost:5000/api

# WebSocket URL for real-time notifications (optional, defaults to API URL)
REACT_APP_SOCKET_URL=http://localhost:5000

# Google Maps API key (for location services)
REACT_APP_GOOGLE_MAPS_API_KEY=your_google_maps_api_key

# Stripe publishable key (for payment UI elements)
REACT_APP_STRIPE_PUBLISHABLE_KEY=pk_test_your_stripe_publishable_key

# Analytics tracking ID (optional)
REACT_APP_GA_TRACKING_ID=UA-XXXXXXXXX-X

# Feature flags (optional)
REACT_APP_ENABLE_CHAT_FEATURE=false
REACT_APP_ENABLE_REVIEWS=true

# Build configuration
REACT_APP_ENV=development
GENERATE_SOURCEMAP=true

# Image CDN base URL (optional)
REACT_APP_IMAGE_CDN_URL=https://cdn.example.com/images

# App version (auto-populated from package.json in build process)
REACT_APP_VERSION=$npm_package_version
